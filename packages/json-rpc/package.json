{
  "name": "@neongd/json-rpc",
  "version": "0.0.10-alpha.14",
  "description": "Json Rpc types and toolings",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/umd/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "run-p build:types build:code",
    "build:code": "rollup -c",
    "build:types": "tsc -p tsconfig.build.json",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "test": "jest --passWithNoTests",
    "watch": "run-p 'build:types -- -w' 'build:code -- -w'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neo-ngd/neo-dapi-monorepo.git"
  },
  "keywords": [
    "jsonrpc"
  ],
  "author": "neo-ngd <github.com/neo-ngd>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neo-ngd/neo-dapi-monorepo/issues"
  },
  "homepage": "https://github.com/neo-ngd/neo-dapi-monorepo",
  "dependencies": {
    "axios": "^0.21.1",
    "bufferutil": "^4.0.3",
    "events": "^3.3.0",
    "utf-8-validate": "^5.0.5",
    "ws": "^8.2.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.3",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-polyfill-node": "^0.7.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
